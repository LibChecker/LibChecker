<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <string-array name="list_repo">
        <item>GitHub</item>
        <item>Gitee</item>
    </string-array>

    <string-array name="list_repo_value">
        <item>github</item>
        <item>gitee</item>
    </string-array>

    <string-array name="language" translatable="false">
        <item>@string/follow_system</item>
        <item>en</item>
        <item>zh-Hans</item>
        <item>zh-Hant</item>
        <item>ru</item>
        <item>uk</item>
    </string-array>

    <string-array name="language_value" translatable="false">
        <item>SYSTEM</item>
        <item>en-US</item>
        <item>zh-CN</item>
        <item>zh-TW</item>
        <item>ru-RU</item>
        <item>uk-UA</item>
    </string-array>

    <string-array name="list_dark_mode" tools:ignore="InconsistentArrays">
        <item>@string/array_dark_mode_off</item>
        <item>@string/array_dark_mode_on</item>
    </string-array>

    <string-array name="list_dark_mode_value" tools:ignore="InconsistentArrays">
        <item>off</item>
        <item>on</item>
    </string-array>
</resources>
